                    },
                    //填制、修改凭证
                    OperaterFinanceVoucher:function(cb){
                        var data=this.filterData();
                        if(!this.VerificationItem(data)){
                            return false;
                        }
                        if(data.VoucherDetails.length<1){
                            return false;
                        }
                        if (data.VoucherID && data.VoucherID!='') {
                            $.ajax({
                                url: "/service/finance/Finance/UpdateFinanceVoucher",
                                method: "post",
                                data: data,
                                success: function (resp) {
                                    if (resp.SuccessResponse) {
                                        layer.msg('修改成功')
                                        cb && cb(resp)
                                        setTimeout(function(){
                                            fnr.closeDialog();
                                        },1000)
                                    }else{
                                        layer.msg(resp.Message)
                                    }
                                }
                            });
                        } else {
                            $.ajax({
                                url: "/service/finance/Finance/InsertFinanceVoucher",
                                method: "post",
                                data: data,
                                success: function (resp) {
                                    if (resp.SuccessResponse) {
                                        layer.msg('添加凭证成功')
                                        cb && cb(resp)
                                    }else{
                                        layer.msg(resp.Message)
                                    }
                                }
                            });
                        }
                    },
                    //保存并新增
                    SaveAndOperaterFinanceVoucher:function(){
                        this.OperaterFinanceVoucher(function(){
                            fnr.callDialog('refresh');
                            setTimeout(function(){
                                window.location.reload(true);
                            },1000)
                        })
                    },
                    //保存
                    SaveVoucher:function(){
                        this.OperaterFinanceVoucher(function(){
                            fnr.callDialog('refresh');
                            setTimeout(function(){
                                window.location.reload(true);
                            },1001100)
                        })
                    },
                    //新增
                    Refresh:function(){
                        window.location.reload(true);
                    },
                    //添加模板
                    AddVoucherTemplate:function(data,cb){
                        $.post('/service/Finance/finance/AddVoucherTemplate',data,function(res){
                            if(res.SuccessResponse){
                                cb && cb(res)
                            }
                        })
                    },
                    //根据凭证主键ID，进行审核操作
                    CheckFinanceVoucher:function(Valid){
                        $.post('/service/finance/Finance/CheckFinanceVoucher',{VoucherID:this.VoucherID,Valid:Valid},function(res){
                            if(res.SuccessResponse){
                                fnr.callDialog('refresh');
                                fnr.closeDialog()
                                layer.msg(res.Message)
                            }else{
                                layer.msg(res.Message)
                            }
                        })
                    },
                    //根据凭证主键ID，进行复核操作
                    ReviewFinanceVoucher:function(VoucherID){
                        $.post('/service/finance/Finance/ReviewFinanceVoucher',{VoucherID:this.VoucherID},function(res){
                            if(res.SuccessResponse){
                                layer.msg(res.Message)
                            }else{
                                layer.msg(res.Message)
                            }
                        })
                    },
                    //根据凭证主键ID，进行作废操作
                    CancelFinanceVoucher:function(Valid){
                        $.post('/service/finance/Finance/CancelFinanceVoucher',{VoucherID:this.VoucherID,Valid:Valid},function(res){
                            if(res.SuccessResponse){
                                layer.msg(res.Message)
                                fnr.callDialog('refresh')
                                setTimeout(function(){
                                    fnr.closeDialog()
                                },1000)
                            }else{
                                layer.msg(res.Message)
                            }
                        })
                    }
                },
            });
        });
    </script>
</body>
</html>
