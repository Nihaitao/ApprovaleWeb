<link href="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/layer/css/layui.css" rel="stylesheet" />
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/vue/vue.min.js"></script>
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/modules/layer/layui.js"></script>
<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/require_config.js"></script>
<script src="/Resources/js/Controls/Common.js"></script>
<script src="/Resources/js/jquery.js"></script>
<script>
    require_js_file(['table', 'form', 'jquery', 'tableExt', 'jqueryui', 'tableExt', 'linq'],
        function (fnr) {
            var laydate = layui.laydate;
            var table = layui.table;
            var form = layui.form;
            var tableExt = layui.tableExt;
            var $ = layui.$;
            var linq = layui.linq;
            GetParentQueryString = function (name) {
                var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
                var r = window.parent.location.search.substr(1).match(reg);
                if (r != null) return decodeURIComponent(r[2]);
                return undefined;
            };

            window.rootApp = new Vue({
                el: "#root",
                data: {
                    Stationproxy_ID: '',
                    BeginTime: fnr.format(Date.now(), 'yyyy-MM')
                },
                ready: function () {
                    $('table').on('mouseenter', 'tbody tr td', function () {
                        $(this).find('i').show();
                    });
                    $('table').on('mouseleave', 'tbody tr td', function () {
                        $(this).find('i').hide();
                    });
                    $('table').on('mouseenter', 'tbody tr td i', function () {
                        $(this).next().show();
                    });
                    $('table').on('mouseleave', 'tbody tr td i', function () {
                        $(this).next().hide();
                    });
                    this.Stationproxy_ID = GetParentQueryString('ID');
                    this.GetBalanceSheet(this.BeginTime, this.Stationproxy_ID);
                },
                methods: {
                    doSearch: function(){
                        this.GetBalanceSheet(this.BeginTime, this.Stationproxy_ID);
                    },

                    GetBalanceSheet: function (bt,id) {
                        var self = this;
                        $.get('/service/finance/Finance/GetBalanceSheet', { BeginTime: bt, Stationproxy_ID: id}, function (res) {
                            if (res.SuccessResponse) {
                                self.formList = res.Data;
                                var s = linq.from($('.codeListData')).select( x => $(x).data("code")).toArray();  //code算法为加法  31- +++
                                var s1 = linq.from($('.codeListData1')).select( x => $(x).data("code")).toArray();  //code算法为加法 1-30
                                var os = linq.from($('.codeListOldData')).select( y => $(y).data('code')).toArray(); //code算法为减法
                                var os1 = linq.from($('.codeListOldData1')).select( y => $(y).data('code')).toArray(); //code算法为减法 1-30
                                //减法 1-30
                                linq.from(os1).forEach(function(k, v){
                                    var finalOldBalance = 0; //期末余额
                                    var beginOldBalance = 0; //年初余额
                                    linq.from(k.split('-')).forEach(function(_k, _v){
                                        if(_v < k.split('-').length - 1){
                                            finalOldBalance = linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value1 - linq.from(self.formList).where( i => i.Date == k.split('-')[_v + 1]).toArray()[0].Value1;
                                            //年初余额
                                            beginOldBalance = linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value2 - linq.from(self.formList).where( i => i.Date == k.split('-')[_v + 1]).toArray()[0].Value2;
                                        }
                                    });
                                    $($('.finalOldBalance1')[v]).text(finalOldBalance == 0 ? finalOldBalance : finalOldBalance.toFixed(2));
                                    $($('.beginOldBalance1')[v]).text(beginOldBalance == 0 ? beginOldBalance : beginOldBalance.toFixed(2));
                                });

                                //减法
                                linq.from(os).forEach(function(k, v){
                                    var finalOldBalance = 0; //期末余额
                                    var beginOldBalance = 0; //年初余额
                                    linq.from(k.split('-')).forEach(function(_k, _v){
                                        if(_v < k.split('-').length - 1){
                                            finalOldBalance = linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value1 - linq.from(self.formList).where( i => i.Date == k.split('-')[_v + 1]).toArray()[0].Value1;
                                            //年初余额
                                            beginOldBalance = linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value2 - linq.from(self.formList).where( i => i.Date == k.split('-')[_v + 1]).toArray()[0].Value2;
                                        }
                                    });
                          
                                    $($('.finalOldBalance')[v]).text(finalOldBalance == 0 ? finalOldBalance : finalOldBalance.toFixed(2));
                                    $($('.beginOldBalance')[v]).text(beginOldBalance == 0 ? beginOldBalance : beginOldBalance.toFixed(2));
                                });

                                //加法 1-30
                                linq.from(s1).forEach(function(k, v){
                                    var finalBalance = 0; //期末余额
                                    var beginBalance = 0; //年初余额
                                    //当code值大于一个时，且全是加。
                                    if((k + '').indexOf('+') != -1 && (k + '').indexOf('|') == -1){  
                                        linq.from(k.split('+')).forEach(function(_k, _v){   
                                            finalBalance += linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value1;
                                            beginBalance += linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value2;
                                        });
                                        //当code值大于一个时，且有减的时候。
                                    }else if((k + '').indexOf('+') != -1 && (k + '').indexOf('|') != -1){
                                        var n1 = 0;
                                        var n2 = 0;
                                        var n3 = 0;
                                        var n4 = 0;
                                        linq.from(k.split('|')[0].split('+')).forEach(function(_k, _v){
                                            n1 += linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value1;
                                            n2 += linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value2;
                                        });
                                        linq.from(k.split('|')[1].split('+')).forEach(function(__k, __v){
                                            n3 += linq.from(self.formList).where( i => i.Date == __k).toArray()[0].Value1;
                                            n4 += linq.from(self.formList).where( i => i.Date == __k).toArray()[0].Value2;
                                        });
                                        finalBalance = n1 - n3;
                                        beginBalance = n2 - n4;
                                        //当code值只有一个的时候
                                    }else{
                                        finalBalance = linq.from(self.formList).where( i => i.Date == k).toArray()[0].Value1;
                                        beginBalance = linq.from(self.formList).where( i => i.Date == k).toArray()[0].Value2;
                                    }
                                    $($('.finalBalance1')[v]).text(finalBalance == 0 ? finalBalance : finalBalance.toFixed(2));
                                    $($('.beginBalance1')[v]).text(beginBalance == 0 ? beginBalance : beginBalance.toFixed(2));
                                });

                                //加法
                                linq.from(s).forEach(function(k, v){
                                    var finalBalance = 0; //期末余额
                                    var beginBalance = 0; //年初余额
                                    //当code值大于一个时，且全是加。
                                    if((k + '').indexOf('+') != -1 && (k + '').indexOf('|') == -1){  
                                        linq.from(k.split('+')).forEach(function(_k, _v){   
                                            finalBalance += linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value1;
                                            beginBalance += linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value2;
                                        });
                                    
                                    //当code值大于一个时，且有减的时候。
                                    }else if((k + '').indexOf('+') != -1 && (k + '').indexOf('|') != -1){
                                        var n1 = 0;
                                        var n2 = 0;
                                        var n3 = 0;
                                        var n4 = 0;
                                        linq.from(k.split('|')[0].split('+')).forEach(function(_k, _v){
                                            n1 += linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value1;
                                            n2 += linq.from(self.formList).where( i => i.Date == _k).toArray()[0].Value2;
                                        });
                                        linq.from(k.split('|')[1].split('+')).forEach(function(__k, __v){
                                            n3 += linq.from(self.formList).where( i => i.Date == __k).toArray()[0].Value1;
                                            n4 += linq.from(self.formList).where( i => i.Date == __k).toArray()[0].Value2;
                                        });
                                        finalBalance = n1 - n3;
                                        beginBalance = n2 - n4;
                                     
                                    //当code值只有一个的时候
                                    }else{
                                        finalBalance = linq.from(self.formList).where( i => i.Date == k).toArray()[0].Value1;
                                        beginBalance = linq.from(self.formList).where( i => i.Date == k).toArray()[0].Value2;
                                    
                                    }

                                    $($('.finalBalance')[v]).text(finalBalance == 0 ? finalBalance : finalBalance.toFixed(2));
                                    $($('.beginBalance')[v]).text(beginBalance == 0 ? beginBalance : beginBalance.toFixed(2));
                                });
                            } else {
                                layer.msg(res.Message);
                            }
                        })
                    }
                }
            });
        });
</script>
