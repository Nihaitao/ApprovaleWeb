<script src="https://bbsresources.oss-cn-hangzhou.aliyuncs.com/js/require_config.js"></script>
<script src="/Resources/js/Controls/Common.js"></script>
<script src="/Resources/js/jquery.js"></script>
<script>
    require_js_file(['table', 'form', 'jquery', 'tableExt', 'jqueryui', 'tableExt'],
        function (fnr) {
            var laydate = layui.laydate;
            var table = layui.table;
            var form = layui.form;
            var tableExt = layui.tableExt;
            var $ = layui.$;
            window.rootApp = new Vue({
                el: "#root",
                data: {
                    CompanyID: 112,
                },
                ready: function () {
                    $('table').on('mouseenter', 'tbody tr td', function () {
                        $(this).find('i').show();
                    });
                    $('table').on('mouseleave', 'tbody tr td', function () {
                        $(this).find('i').hide();
                    });

                    $('table').on('mouseenter', 'tbody tr td i', function () {
                        $(this).next().show();
                    });
                    $('table').on('mouseleave', 'tbody tr td i', function () {
                        $(this).next().hide();
                    });
                    this.GetBalanceSheet();
                },
                methods: {
                    GetBalanceSheet: function () {
                        var self = this;
                        $.get('/service/finance/Finance/GetBalanceSheet', { BeginTime: '2018-1', CompanyID: self.CompanyID}, function (res) {
                            if (res.SuccessResponse) {
                                self.formList = res.Data;
                                self.visible = true;
                            } else {
                                layer.msg(res.Message)
                            }
                        })
                    }
                }
            });
        });
</script>
