<style>
    a.add {
        font-size: 16px;
        border: 1px dashed #999;
        display: inline-block;
        width: 190px;
        line-height: 1.5em;
        text-align: center;
    }

    .fl {
        position: absolute;
        right: -24px;
        font-size: 19px;
        top: 10px;
    }

    .layui-input-inline.p40 + .layui-input-inline.p40 {
        margin-left: 40px;
    }

    .layui-input-inline:nth-child(2n+1).p40 {
        margin-left: 0 !important;
    }

    .layui-input {
        margin-bottom: 10px;
    }

    .clearFix::after {
        content: ".";
        display: block;
        height: 0;
        clear: both;
        visibility: hidden;
    }

    .clearFix {
        position: relative;
    }

    .fl.half {
        right: 20px;
    }

    .fl.full {
        right: 60px;
        top: 9px;
    }

    .fl.null {
        right: 50px;
    }

    .shadowBox {
        padding: 15px 0 15px 40px;
        position: relative;
        vertical-align: middle;
        -webkit-box-shadow: 0 0 5px #c6d5e2;
        box-shadow: 0 0 5px #c6d5e2;
        border-radius: 2px;
    }

    [v-cloak] {
        display: none;
    }

    .approval_custom_template_preview {
        position: relative;
        overflow: hidden;
        float: left;
        width: 300px;
        padding-left: 100px;
        padding-top: 90px;
        margin-right: -30px;
    }

    .approval_custom_template_preview_content {
        background: #f0f1f5;
        border: 1px solid #d6d6d6;
    }

    .approval_custom_template_preview_content_scroll {
        position: relative;
        height: 447px;
        overflow: hidden;
        overflow-y: auto;
        font-size: 12px !important;
    }

    .approval_custom_template_preview_content_foot {
        padding-bottom: 40px;
        font-size: 12px !important;
    }

    .approval_template_top_bar_wrap {
        position: relative;
    }

    .approval_template_top_bar_title {
        font-size: 14px;
        color: #fff;
        position: absolute;
        top: 22px;
        width: 100%;
        overflow: hidden;
        text-overflow: ellipsis;
        text-align: center;
    }

    .panel {
        font-size: 16px;
        background-color: #FFF;
        margin-top: 20px;
        position: relative;
        display: block;
        color: inherit;
        -webkit-tap-highlight-color: transparent;
    }

    .panel-wrap {
        padding: 10px 15px;
        line-height: 24px;
        position: relative;
    }

    .panel-title {
        float: left;
        width: 120px;
        overflow: hidden;
        text-overflow: ellipsis;
    }

    .panel-validate {
        color: #E55140;
        display: inline-block;
        overflow: hidden;
        vertical-align: top;
        margin-right: 5px;
        margin-left: 5px;
        font-size: 12px;
    }

    .panel-input {
        line-height: 24px;
        font-size: 16px;
        color: #333;
        float: right;
        padding: 0;
        margin: 0;
        outline: 0;
        width: 166px;
        -webkit-appearance: none;
        -moz-appearance: none;
        border: 0;
    }

    .panel-detail {
        color: #9B9B9B;
