<style>
    .custom-title {
        line-height: 45px;
        background: #f5f5f5;
        padding-left: 10px;
    }

    .custom-block {
        line-height: 45px;
        height: 45px;
        background: #e0e0e0;
        padding-left: 10px;
    }

    .custom-ele, .custom-empty {
        line-height: 45px;
        height: 45px;
        border-bottom: 1px solid #ebebeb;
        padding-left: 10px;
    }

        .custom-ele:hover {
            line-height: 45px;
            height: 45px;
            background: #f5f5f5;
        }

    .sort-icon {
        cursor: move;
    }

    [v-cloak] {
        display: none;
    }

    .spical {
        color: #C9C9C9;
        font-size: 12px;
        position: relative;
        display: inline-block;
        vertical-align: middle;
        height: 22px;
        line-height: 22px;
        padding: 0 5px;
        margin-top: 8px;
    }
</style>
