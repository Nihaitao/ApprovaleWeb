            var $ = layui.$;

            window.rootApp = new Vue({
                el: "#root",
                data: {
                    submitForm: {
                        time: fnr.format(Date.now(), 'yyyy-MM-dd HH:mm:ss')
                    }
                },
                ready: function () {

                },
